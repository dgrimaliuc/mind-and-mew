{
  "name": "neon-wave",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "reauth": "firebase login --reauth",
    "deploy": "npm run build && firebase deploy",
    "channel:create": "npm run build && firebase hosting:channel:create $1 --expires 7d",
    "channel:deploy": "npm run build && firebase hosting:channel:deploy $1 --expires 7d",
    "channel:delete": "firebase hosting:channel:delete $1 --force",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@mui/icons-material": "^6.4.0",
    "@mui/material": "^6.4.0",
    "@reduxjs/toolkit": "^2.1.0",
    "@tanstack/react-query": "^5.64.2",
    "firebase": "^11.2.0",
    "framer-motion": "^12.0.0",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.21.2",
    "react-scripts": "^5.0.1",
    "sass": "^1.83.4"
  },
