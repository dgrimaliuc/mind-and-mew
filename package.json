    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.0",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "^18.2.0",
    "sass": "^1.83.4",
    "sass-loader": "^16.0.4",
    "ts-jest": "^29.1.2",
